<script type="text/javascript">

function X(port_id, transceiver_id, connect_type, vendor_name, vendor_pn, wavelength, temp_h_alarm, temp_l_alarm)

{

    var i;

    var tbtr;

    var narr;

    var tbtd;

    var info = new Array(port_id, transceiver_id, connect_type, vendor_name, vendor_pn, wavelength);

    var reg = new RegExp("\n","g");

	

	var obj = document.getElementById("msg_note");

    obj.style.display = "none";

	

    tbtr = document.getElementById("table_port").insertRow(-1);

    

    tbtr.classname = "td7";

    tbtr.height = "30";

    tbtr.setAttribute("height", "30");

    tbtr.setAttribute("class", "td7");

    tbtr.setAttribute("className", "td7");

    

    narr = info.length;

    for(i=0;i<narr;i++)

    {

        tbtd = document.createElement("td");

     

        tbtd.align = "center";

        tbtd.setAttribute("class", "td2");  

        tbtd.setAttribute("className", "td2");  

        tbtr.appendChild(tbtd);

    }

    

    for(i=0; i<narr; i++)

    {

        info[i] = info[i].replace(reg, "\0");

        tbtr.cells[i].innerHTML = info[i];

    }

}



function refreshpage()

{

	location.href='transceiver_basic.asp';

}


function test_ajax(){
    $.ajax(
    {
      type:"GET",//通常会用到两种：GET,POST。默认是：GET
      url:"devinfo.txt",//(默认: 当前页地址) 发送请求的地址
      dataType:"html",//预期服务器返回的数据类型。
      beforeSend:beforeSend, //发送请求
      success:callback, //请求成功
      error:error,//请求出错 
      complete:complete//请求完成
    });
}
function error(XMLHttpRequest, textStatus, errorThrown){
    // 通常情况下textStatus和errorThown只有其中一个有值 
    $("#showResult").append("<div>请求出错啦！</div>");
}
function beforeSend(XMLHttpRequest){
    $("#showResult").append("<div><p>加载中...</p><div>");
}
function complete(XMLHttpRequest, textStatus){
    $("#showResult").remove();
}
function callback(msg){
    $("#table_port").append("<table><tr><td>请求成功，回传数:"+msg+"</td><tr></table>");
}

</script>



